{
  "name": "logline",
  "displayName": "LogLine",
  "description": "See the Log the way it needs to be seen.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [{
      "command": "logline.helloWorld",
      "title": "Log Line"
    }],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "logline-container",
          "title": "Log Line",
          "icon": "resources/dep.png"
        }
      ]
    },
    "views": {
      "logline-container": [
        {
          "type": "webview",
          "id": "uniqueViewId",
          "name": "Web View Hello World"
        }        
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.84.0"
  }
}
